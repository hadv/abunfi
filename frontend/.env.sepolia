# Sepolia Testnet Configuration for Security Testing
# Copy this to .env.local for Sepolia testing

# API Configuration
REACT_APP_API_URL=http://localhost:3001/api

# Web3Auth Configuration
REACT_APP_WEB3AUTH_CLIENT_ID=your_web3auth_client_id_here
REACT_APP_WEB3AUTH_NETWORK=sapphire_devnet

# Sepolia Blockchain Configuration
REACT_APP_CHAIN_ID=11155111
REACT_APP_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID

# Contract Addresses (Update these after deployment)
# Core contracts
REACT_APP_VAULT_CONTRACT_ADDRESS=0x...
REACT_APP_STRATEGY_MANAGER_ADDRESS=0x...

# EIP-7702 Gasless Transaction contracts (Security Testing)
REACT_APP_SMART_ACCOUNT_ADDRESS=0x...
REACT_APP_EIP7702_BUNDLER_ADDRESS=0x...
REACT_APP_EIP7702_PAYMASTER_ADDRESS=0x...

# Token contracts
REACT_APP_USDC_CONTRACT_ADDRESS=0x...

# Security Testing Configuration
REACT_APP_ENABLE_SECURITY_TESTING=true
REACT_APP_SECURITY_TEST_MODE=true

# Test Accounts (Update these after deployment)
REACT_APP_TEST_WHITELISTED_ACCOUNT=0x...
REACT_APP_TEST_RESTRICTED_ACCOUNT=0x...
REACT_APP_TEST_STANDARD_ACCOUNTS=0x...,0x...,0x...

# Rate Limiting Configuration for Testing
REACT_APP_DEFAULT_DAILY_GAS_LIMIT=0.1
REACT_APP_WHITELISTED_DAILY_GAS_LIMIT=0.2
REACT_APP_DEFAULT_PER_TX_GAS_LIMIT=0.01
REACT_APP_WHITELISTED_PER_TX_GAS_LIMIT=0.02
REACT_APP_DEFAULT_DAILY_TX_LIMIT=50
REACT_APP_WHITELISTED_DAILY_TX_LIMIT=100

# WebSocket Configuration for Real-time Monitoring
REACT_APP_WS_URL=ws://localhost:3001
REACT_APP_ENABLE_REAL_TIME_MONITORING=true

# Development Features
REACT_APP_ENABLE_DEV_TOOLS=true
REACT_APP_SHOW_SECURITY_DASHBOARD=true
REACT_APP_ENABLE_SECURITY_NOTIFICATIONS=true

# Logging
REACT_APP_LOG_LEVEL=debug
REACT_APP_ENABLE_SECURITY_LOGGING=true
