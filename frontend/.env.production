# Frontend Production Environment Variables
# These will be embedded in the build, so don't include sensitive data

# API Configuration
REACT_APP_API_URL=https://your-domain.com/api

# Web3 Configuration
REACT_APP_WEB3AUTH_CLIENT_ID=your_web3auth_client_id_here
REACT_APP_CHAIN_ID=11155111
REACT_APP_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID

# Smart Contract Addresses (update after deployment)
REACT_APP_VAULT_CONTRACT_ADDRESS=0x...

# Application Configuration
REACT_APP_APP_NAME=Abunfi
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Optional: Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Optional: Error Tracking
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
